          {
            "editorMode": "code",
            "format": "table",
            "rawQuery": true,
            "rawSql": "SELECT \n    EXTRACT(HOUR FROM uq.timestamp) AS hour_of_day,\n    COUNT(*) AS total_queries,\n    SUM(CASE WHEN d.category = 'Business' THEN 1 ELSE 0 END) AS Business,\n    SUM(CASE WHEN d.category = 'Search Engine' THEN 1 ELSE 0 END) AS \"Search Engine\",\n    SUM(CASE WHEN d.category = 'Social Media' THEN 1 ELSE 0 END) AS \"Social Media\",\n    SUM(CASE WHEN d.category = 'Entertainment' THEN 1 ELSE 0 END) AS Entertainment,\n    SUM(CASE WHEN d.category = 'Shopping' THEN 1 ELSE 0 END) AS Shopping,\n    SUM(CASE WHEN d.category = 'Gambling' THEN 1 ELSE 0 END) AS Gambling,\n    SUM(CASE WHEN d.category = 'Technology' THEN 1 ELSE 0 END) AS Technology,\n    SUM(CASE WHEN d.category = 'Professional Networking' THEN 1 ELSE 0 END) AS \"Professional Networking\",\n    SUM(CASE WHEN d.category = 'Education' THEN 1 ELSE 0 END) AS Education,\n    SUM(CASE WHEN d.category = 'News' THEN 1 ELSE 0 END) AS News,\n    SUM(CASE WHEN d.category = 'Finance' THEN 1 ELSE 0 END) AS Finance,\n    SUM(CASE WHEN d.category = 'Sports' THEN 1 ELSE 0 END) AS Sports,\n    SUM(CASE WHEN d.category = 'Adult Content' THEN 1 ELSE 0 END) AS \"Adult Content\",\n    SUM(CASE WHEN d.category IS NULL OR d.category = '' THEN 1 ELSE 0 END) AS Uncategorized\nFROM \n    user_query uq\nJOIN \n    domains d ON uq.domain_id = d.id\nJOIN\n    user_connections uc ON uq.connection_id = uc.id\nJOIN\n    users u ON uc.user_id = u.id\nWHERE \n    uq.timestamp >= $__timeFrom() \n    AND uq.timestamp <= $__timeTo()\n    AND d.name IN ($Domain)\n    AND d.category IN ($Category)\n    AND u.browsing_profile IN ($BrowsingProfile)\nGROUP BY \n    hour_of_day\nORDER BY \n    hour_of_day;\n",
