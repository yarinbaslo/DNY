            "rawSql": "SELECT\n  uc.region AS \"Region\",\n  d.category AS \"Domain Category\",\n  COUNT(uq.id) AS \"Query Count\"\nFROM\n  user_query uq\nJOIN\n  user_connections uc ON uq.connection_id = uc.id\nJOIN\n  users u ON uc.user_id = u.id\nJOIN\n  domains d ON uq.domain_id = d.id\nWHERE\n  $__timeFilter(uq.timestamp)\n  AND d.category IS NOT NULL\n  AND d.name IN ($Domain)\n  AND d.category IN ($Category)\n  AND u.browsing_profile IN ([[BrowsingProfile]])\nGROUP BY\n  uc.region, d.category\nORDER BY\n  \"Query Count\" DESC\nLIMIT 20;\n",
